<?xml version="1.0" encoding="UTF-8"?>
<cockatrice_deck version="1">
  <deckname><%= @session_user.username %>'s cards</deckname>
  <comments>Draftin' Cards Export</comments>
  <zone name="main">
    <% @session_user.cards_by_name.each do |card_name, count| %>
      <card number="<%= count %>" price="0" name="<%= card_name %>"/>
    <% end %>
  </zone>
</cockatrice_deck>
